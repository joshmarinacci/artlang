@board("pygamer")

print("color starting")
@type("start",setup)
fun setup() {
    print("in the setup")
    screen.fill(BLUE)
    wait(1)
    screen.fill(GREEN)
    wait(1)
    screen.fill(BLACK)
}

@type("event",buttons,check)
fun check() {
    print("inside check")
    event = buttons.events.get()
    if event print("event happened", event.key_number, event.pressed, event.released)
}
